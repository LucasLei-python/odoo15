<odoo>
<record model="ir.cron" id="email_u8">
        <field name="name">Send Email To U8 EF</field>
        <field name="model_id" ref="model_xlcrm_account"/>
        <field name="state">code</field>
        <field name="code">model.task()</field>
        <field name='interval_number'>1</field>
        <field name='interval_type'>weeks</field>
        <field name="numbercall">-1</field>
        <field name="doall" eval="True" />
</record>
<record model="ir.cron" id="synchronization_cus_u8">
<field name="name">Synchronization Cus To U8 </field>
<field name="model_id" ref="model_xlcrm_u8_customer"/>
<field name="state">code</field>
<field name="code">model.task()</field>
<field name='interval_number'>30</field>
<field name='interval_type'>minutes</field>
<field name="numbercall">-1</field>
<field name="doall" eval="True" />
</record>
</odoo>